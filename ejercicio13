def main():
    num1= int(input("Ingrese numero1: "))
    num2= int(input("Ingrese numero2: "))
    suma= num1 + num2
    resta= num1 - num2
    multi= num1 * num2
    divi= num1 / num2
    print(f"la suma es: {suma}, la resta es: {resta}, la multiplicacion es: {multi} y la division es:{divi}")
main()